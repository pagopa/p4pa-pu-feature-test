<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<FlussoRiversamento xmlns="http://www.digitpa.gov.it/schemas/2011/Pagamenti/">
    <versioneOggetto>1.0</versioneOggetto>
    <identificativoFlusso>{identificativo_flusso}</identificativoFlusso>
    <dataOraFlusso>{data_ora_flusso}</dataOraFlusso>
    <identificativoUnivocoRegolamento>{identificativo_univoco_regolamento}</identificativoUnivocoRegolamento>
    <dataRegolamento>{data_regolamento}</dataRegolamento>
    <istitutoMittente>
