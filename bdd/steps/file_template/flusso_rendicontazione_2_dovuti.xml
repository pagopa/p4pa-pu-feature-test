            <tipoIdentificativoUnivoco>{tipo_identificativo_univoco_mittente}</tipoIdentificativoUnivoco>
            <codiceIdentificativoUnivoco>{codice_identificativo_univoco_mittente}</codiceIdentificativoUnivoco>
        </identificativoUnivocoMittente>
        <denominazioneMittente>{denominazione_mittente}</denominazioneMittente>
