        <codiceEsitoSingoloPagamento>{codice_esito_singolo_pagamento}</codiceEsitoSingoloPagamento>
        <dataEsitoSingoloPagamento>{data_esito_singolo_pagamento}</dataEsitoSingoloPagamento>
    </datiSingoliPagamenti>
    <datiSingoliPagamenti>
        <identificativoUnivocoVersamento>{iuv_2}</identificativoUnivocoVersamento>
        <identificativoUnivocoRiscossione>{iur_2}</identificativoUnivocoRiscossione>
        <indiceDatiSingoloPagamento>{indice_dati_singolo_pagamento_2}</indiceDatiSingoloPagamento>
        <singoloImportoPagato>{singolo_importo_pagato_2}</singoloImportoPagato>
        <codiceEsitoSingoloPagamento>{codice_esito_singolo_pagamento}</codiceEsitoSingoloPagamento>
        <dataEsitoSingoloPagamento>{data_esito_singolo_pagamento}</dataEsitoSingoloPagamento>
    </datiSingoliPagamenti>
