            <codiceIdentificativoUnivoco>{codice_identificativo_univoco_ricevente}</codiceIdentificativoUnivoco>
        </identificativoUnivocoRicevente>
    </istitutoRicevente>
    <numeroTotalePagamenti>{numero_totale_pagamenti}</numeroTotalePagamenti>
    <importoTotalePagamenti>{importo_totale_pagamenti}</importoTotalePagamenti>
